{
  "name": "propaganistas/browsershot-lambda",
  "conflict": {
    "wnx/sidecar-browsershot": "*"
  },
  "require": {
    "php": "^8.0",
    "aws/aws-sdk-php": "^3.300",
    "illuminate/config": "^10.0",
    "illuminate/container": "^10.0",
    "illuminate/filesystem": "^10.0",
    "illuminate/support": "^10.0",
    "spatie/laravel-pdf": "^1.4",
    "spatie/browsershot": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Propaganistas\\BrowsershotLambda\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Propaganistas\\BrowsershotLambda\\BrowsershotLambdaServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
